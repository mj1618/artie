{
  "name": "composure",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:next": "next dev",
    "dev:convex": "convex dev",
    "prebuild": "mkdir -p ./builds/${BUILD_ID}/cache && [ -d .next-cache ] && cp -r .next-cache/* ./builds/${BUILD_ID}/cache/ 2>/dev/null || true",
    "build": "BUILD_DIR=./builds/${BUILD_ID} next build",
    "postbuild": "mkdir -p .next-cache && cp -r ./builds/${BUILD_ID}/cache/* .next-cache/ 2>/dev/null || true && cp -r ./builds/${BUILD_ID}/static ./builds/${BUILD_ID}/standalone/builds/${BUILD_ID}/static && cp -r ./public ./builds/${BUILD_ID}/standalone/public",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.44",
    "@ai-sdk/google": "^3.0.29",
    "@ai-sdk/openai": "^3.0.29",
    "@clerk/nextjs": "^6.38.0",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@octokit/rest": "^22.0.1",
    "@webcontainer/api": "^1.6.1",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "ai": "^6.0.86",
    "convex": "^1.31.7",
    "diff": "^8.0.3",
    "lucide-react": "^0.574.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.22.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/diff": "^7.0.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "^8.0.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}